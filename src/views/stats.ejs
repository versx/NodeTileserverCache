<!DOCTYPE html>
<html lang="en">
    <%- include('header'); %>
    <%- include('navbar'); %>
    <br><br><br>
    <body class="w-75" style="float: none; margin: 0 auto;">
        <h4 class="text-center"><b>Tile Cache Hit-Rate</b></h4>
        <div class="card-body">
            <table id="table" class="table table-striped table-bordered table-hover dt-responsive nowrap" style="position: center; width:100%">
                <thead class="thead-dark">
                    <tr>
                        <th>Style</th>
                        <th>Hits</th>
                        <th>Total</th>
                        <th>Percentage</th>
                    </tr>
                </thead>
                <tbody>
                <% if (tileHits) {
                    tileHits.forEach(hit => {
                %>
                    <tr>
                        <td><%= hit.style %></td>
                        <td><%= hit.hit %></td>
                        <td><%= hit.total %></td>
                        <td><%= hit.percentage %>%</td>
                    </tr>
                <%
                    });
                }
                %>
                </tbody>
            </table>
        </div>
        <h4 class="text-center"><b>Static Map Cache Hit-Rate</b></h4>
        <div class="card-body">
            <table id="table" class="table table-striped table-bordered table-hover dt-responsive nowrap" style="position: center; width:100%">
                <thead class="thead-dark">
                    <tr>
                        <th>Style</th>
                        <th>Hits</th>
                        <th>Total</th>
                        <th>Percentage</th>
                    </tr>
                </thead>
                <tbody>
                    <% if (staticHits) {
                        staticHits.forEach(hit => {
                    %>
                        <tr>
                            <td><%= hit.style %></td>
                            <td><%= hit.hit %></td>
                            <td><%= hit.total %></td>
                            <td><%= hit.percentage %>%</td>
                        </tr>
                    <%
                        });
                    }
                    %>
                </tbody>
            </table>
        </div>
        <h4 class="text-center"><b>Static Map with Marker Cache Hit-Rate</b></h4>
        <div class="card-body">
            <table id="table" class="table table-striped table-bordered table-hover dt-responsive nowrap" style="position: center; width:100%">
                <thead class="thead-dark">
                    <tr>
                        <th>Style</th>
                        <th>Hits</th>
                        <th>Total</th>
                        <th>Percentage</th>
                    </tr>
                </thead>
                <tbody>
                    <% if (staticMarkerHits) {
                        staticMarkerHits.forEach(hit => {
                    %>
                        <tr>
                            <td><%= hit.style %></td>
                            <td><%= hit.hit %></td>
                            <td><%= hit.total %></td>
                            <td><%= hit.percentage %>%</td>
                        </tr>
                    <%
                        });
                    }
                    %>
                </tbody>
            </table>
        </div>
        <h4 class="text-center"><b>Marker Cache Hit-Rate</b></h4>
        <div class="card-body">
            <table id="table" class="table table-striped table-bordered table-hover dt-responsive nowrap" style="position: center; width:100%">
                <thead class="thead-dark">
                    <tr>
                        <th>Style</th>
                        <th>Hits</th>
                        <th>Total</th>
                        <th>Percentage</th>
                    </tr>
                </thead>
                <tbody>
                    <% if (markerHits) {
                        markerHits.forEach(hit => {
                    %>
                        <tr>
                            <td><%= hit.style %></td>
                            <td><%= hit.hit %></td>
                            <td><%= hit.total %></td>
                            <td><%= hit.percentage %>%</td>
                        </tr>
                    <%
                        });
                    }
                    %>
                </tbody>
            </table>
        </div>
        <h4 class="text-center"><b>Available Styles</b></h4>
        <div class="card-body">
            <table id="table" class="table table-striped table-bordered table-hover dt-responsive nowrap" style="position: center; width:100%">
                <thead class="thead-dark">
                    <tr>
                        <th>Id</th>
                        <th>Name</th>
                        <th>Url</th>
                        <th>Version</th>
                    </tr>
                </thead>
                <tbody>
                    <% if (styles) {
                        styles.forEach(style => {
                    %>
                        <tr>
                            <td><%= style.id %></td>
                            <td><%= style.name %></td>
                            <td><a href="<%= style.url %>" target="_blank">Style Url</a></td>
                            <td><%= style.version %></td>
                        </tr>
                    <%
                        });
                    }
                    %>
                </tbody>
            </table>
        </div>
    </body>
</html>